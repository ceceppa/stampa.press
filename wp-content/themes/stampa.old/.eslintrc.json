{
  "rules": {
    "semi": 2,
    "no-param-reassign": 0,
    "arrow-parens": ["error", "as-needed"],
    "no-plusplus": "off",
    "import/no-unresolved": "off",
    "import/extensions": "off",
    "prefer-arrow-callback": "off",
    "func-names": "off",
    "no-use-before-define": "off"
  },
  "env": {
    "browser": true,
    "es6": true
  },
  "extends": ["eslint-config-airbnb"],
  "globals": {
    "jQuery": true,
    "google": true,
    "stella": true
  }
}
