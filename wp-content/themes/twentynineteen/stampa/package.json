    "eslint-plugin-react-hooks": "^1.6.0",
    "parcel-bundler": "^1.12.3",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.2",
    "postcss-preset-env": "^6.6.0",
    "postcss-scss": "^2.0.0",
