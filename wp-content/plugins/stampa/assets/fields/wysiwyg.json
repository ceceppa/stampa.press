{
  "group": "Fields",
  "id": "wysiwyg",
  "data": {
    "label": "Paragraph",
    "tooltip": "WYSIWYG Editor (<RichText />)",
    "help": "https://github.com/WordPress/gutenberg/tree/master/packages/block-editor/src/components/rich-text",
    "icon": "wysiwyg.svg",
    "html": "{placeholder}",
    "options": [
      {
        "name": "placeholder",
        "type": "text",
        "label": "Placeholder",
        "value": "Write text..."
      }
